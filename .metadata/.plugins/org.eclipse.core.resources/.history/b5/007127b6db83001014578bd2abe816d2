package SWEA;

import java.util.Scanner;

public class D3_5215_햄버거다이어트 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		
		for (int t=1; t<=T; t++) {
			int N = sc.nextInt(); // 재료 수
			int L = sc.nextInt(); // 제한 칼로리
			
			int[] score = new int[N];
			int[] cal = new int[N];
			
			for (int i=0; i<N; i++) {
				score[i] = sc.nextInt();
				cal[i] = sc.nextInt();
			}
			
			
			
		} // tc end
		
	}
}
